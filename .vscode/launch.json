{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutterfrontend",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/flutterfrontend/lib/main.dart"
    },
    {
      "name": "gocollector",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/gocollector"
    },
    {
      "name": "gomqttemitter",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/gomqttemitter"
    },
    {
      "name": "goservice",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/goservice"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "nodemqttemitter",
      "preLaunchTask": "build nodemqttemitter",
      "program": "${workspaceFolder}/nodemqttemitter/src/app.ts",
      "outFiles": ["${workspaceFolder}/nodemqttemitter/dist/**/*.js"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "nodeservice",
      "preLaunchTask": "build nodeservice",
      "program": "${workspaceFolder}/nodeservice/src/app.ts",
      "outFiles": ["${workspaceFolder}/nodeservice/dist/**/*.js"],
      "sourceMaps": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "reactfrontend",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/reactfrontend",
      "runtimeExecutable": "/usr/bin/chromium-browser",
      "runtimeArgs": [
        "--new-window",
        "--user-data-dir=/${workspaceFolder}/reactfrontend/DevProfile",
        "--remote-debugging-port=9222",
        "--disable-background-networking"
      ]
    },
    
  ]
}